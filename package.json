{
  "name": "hayat-fullstack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "react-scripts start",
    "start:backend": "ts-node backend/src/hayatApp.ts",
    "dev": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "dev:backend": "nodemon --exec ts-node backend/src/hayatApp.ts",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@aws-amplify/datastore": "^5.0.56",
    "@aws-cdk/aws-dynamodb": "^1.204.0",
    "@aws-cdk/aws-events-targets": "^1.204.0",
    "@aws-cdk/core": "^1.204.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.679.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-i18next": "^8.1.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-router-dom": "^5.3.3",
    "apollo-server-express": "^3.13.0",
    "aws-amplify": "^6.6.7",
    "aws-cdk-lib": "^2.164.1",
    "aws-sdk": "^2.1348.0",
    "axios": "^1.7.7",
    "concurrently": "^7.6.0",
    "constructs": "^10.4.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.1.0",
    "react-intl": "^6.8.4",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
